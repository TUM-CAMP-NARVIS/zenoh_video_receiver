set(zdepth_version_major 1)
set(zdepth_version_minor 0)
set(zdepth_version ${zdepth_version_major}.${zdepth_version_minor})

set(PACKAGE_VERSION ${zdepth_version})
if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "${zdepth_version_major}")
    if ("${PACKAGE_FIND_VERSION_MINOR}" EQUAL "${zdepth_version_minor}")
        set(PACKAGE_VERSION_EXACT TRUE)
    elseif("${PACKAGE_FIND_VERSION_MINOR}" LESS "${zdepth_version_minor}")
        set(PACKAGE_VERSION_COMPATIBLE TRUE)
    else()
        set(PACKAGE_VERSION_UNSUITABLE TRUE)
    endif()
else()
    set(PACKAGE_VERSION_UNSUITABLE TRUE)
endif()
